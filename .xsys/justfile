FEED_SERVICE:="xsys-feeds.service"

help:
  just --list --unsorted

build:
  cargo build --release
alias b := build

dev:
  cargo watch -cx lbuild
alias d := dev

run CMD:
  cargo run -- {{CMD}}
alias r := run

install: build
  @mkdir -p bin
  cp target/release/xsys bin/xsys
  crontab res/feeds.cron


